	    </b:FluidRow>
	    <b:FluidRow>
	    	<b:incubator.Table ui:field="problemPanel">
		    </b:incubator.Table>
	    </b:FluidRow>
	    
	    
	    
	</b:FluidRow>
	
	
	
</ui:UiBinder>