	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>

	.table {
		text-align:center;
	}
	    
	.title {
		font-size:24px;
		font-family:Consolas;
	}
		
	magSpan {
		background-color: inherit;   
		color: black;                
		font-size: 16px; 
		font-family: Consolas;
		min-width:40em;
	}

	
	</ui:style>
	
	<b:FluidRow>
	    <b:FluidRow>
