<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:webEditor.client.view">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel>
		<g:Label>Username: </g:Label><g:TextBox ui:field="username" />
		<g:Label>Password: </g:Label><g:PasswordTextBox ui:field="password" />
		<g:Button styleName="{style.important}" ui:field="loginButton" />
		<my:WEAnchor ui:field="registerLink"></my:WEAnchor>
	</g:HorizontalPanel>
</ui:UiBinder> 