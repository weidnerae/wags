<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.reviewer{
			width: 95%;
			background-color: #EEEEEE ;
			height: 1200px;	
			border-style: none;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:RichTextArea ui:field="review" enabled="false" styleName="{style.reviewer}"> Reviewer </g:RichTextArea>
	</g:HTMLPanel>
</ui:UiBinder> 