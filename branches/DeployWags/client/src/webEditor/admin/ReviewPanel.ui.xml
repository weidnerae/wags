<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:webEditor.admin">
	<ui:style>
		.title {
			padding-bottom: 8px;
			padding-top: 8px;
			font-size: x-large;
			font-family: helvetica;	
			text-align: center;
			float: left;
			margin-left: 20px;
		}
		.grid{
			height: 100%;
			background-color: #FFFFFF;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:Label styleName="{style.title}">View Grades</g:Label> 
		<g:HorizontalPanel>
			<g:VerticalPanel>
				<g:Button ui:field="btnExTypes" width="100%">Switch to Review</g:Button>
				<my:ButtonPanel ui:field="btnPnlCurrent"/>
				<my:ButtonPanel ui:field="btnPnlReview"/>
			</g:VerticalPanel>
			<g:Grid ui:field="grdGrades" styleName="{style.grid}"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 