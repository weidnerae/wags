<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:webEditor.admin">
	<ui:style>
		.title {
			padding-bottom: 8px;
			padding-top: 8px;
			font-size: x-large;
			font-family: helvetica;	
			text-align: center;
			margin-top: 20px;
		}
		
		.content {
			height: 100%;
			width: 100%;
		}
		
		.padding {
			padding-right: 5px;
			padding-left: 5px;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" styleName='{style.content}'>
	    <g:east size="560">
				<my:ReviewPanel styleName="{style.padding}" ui:field="rvLogPanel"/>
		</g:east>
		<g:center>
				<my:ReviewPanel styleName="{style.padding}" ui:field="rvMagPanel"/>
		</g:center>
		<g:south size="200">
				<g:FormPanel styleName="{style.padding}" ui:field="formCompReview">
					<g:VerticalPanel>
						<g:Label styleName="{style.title}"> Review All </g:Label>
						<g:SubmitButton ui:field="btnCompReview">Download CSV</g:SubmitButton>
					</g:VerticalPanel>
				</g:FormPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 