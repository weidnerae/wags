<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:webEditor.programming.view">
	
	<ui:style>
        .listBox {
            background-color: #4a6713;
            border-color: #4a6713;
            cursor: pointer;
            color: white;
            font-size: 18px;
            font-family: arial, verdana, san-serif;
            font-weight: bolder;
            top: 1em;
            position: relative;
        }
	
		.applicationBar {
			padding-bottom:0px;
			width: 100%;
			text-align: right; 
			background-color: rgb(50,92,116);
			overflow: visible !important;
        }

        .bottomBordered {
			border: 5px solid black;
			overflow: visible;
 		}

		.anchor { 
			cursor: pointer; 
			position: relative;
    		font-family: arial, verdana, san-serif;
    		font-size: 15px;
    		text-align: center;
            color: white;
            width: 100%;
            height: 100%;
            position:relative;
            margin: 10, auto;
            padding: 0px;
            cursor: pointer;
			top: 0em;
			overflow: visible !important;
		}
		
		.anchor:hover { 
			color: gold; 
		}

		.hello-logout {  
			vertical-align: middle; 
       		padding-right: 5px; 
            display: inline;
			position: relative;
			cursor:pointer;
			overflow: visible !important;
		}
		
		.std-txt { 
			padding-top: 6px 
		}
		
		.title { 
            cursor: pointer;
			font-size: 30px;
			font-weight: heavy;
			color: white;
			z-index: 101;
			position: relative;
			vertical-align: center;
			display: inline;
            float:left;
			text-decoration:none;
			left:0.7em;
			top:0.2em;
		}
		.title:visited {
			color:white;
        }
		.title:hover {
			color:gold;
        }
		ul {
  			text-align: left;
  			display: inline;
  			margin: 0;
  			padding: 15px 4px 17px 0;
  			list-style: none;
			z-index: 1;
		}
		ul li {
  			display: inline-block;
  			margin-right: -4px;
  			position: relative;
  			padding: 15px 15px;
			background: transparent;
  			cursor: pointer;
			
		}
		ul li:hover {
  			background: #444;
  			color: #fff;
			height: 57%;
		}
		ul li ul {
  			padding: 0;
  			position: absolute;
  			top: 48px;
  			left: 0;
  			width: 150px;
  			display: none;
  			opacity: 0;
  			visibility: hidden;
		}
		ul li ul li { 
  			background: #444; 
  			display: block; 
  			color: #fff;
  			text-shadow: 0 -1px 0 #000;
		}
		ul li ul li:hover { 
			background: #555; 
		}
		ul li:hover ul {
  			display: block;
  			opacity: 1;
  			visibility: visible;
		}
			
	</ui:style>

	<g:DockLayoutPanel unit="EM" ui:field="dock">
		<g:north size="6" styleName="{style.bottomBordered}" overflow="visible" >
		    <g:SimplePanel ui:field="north">
			<g:HTMLPanel styleName="{style.applicationBar}">
				<g:Anchor horizontalAlignment="ALIGN_CENTER" styleName="{style.title}" ui:field="Home">WAGS</g:Anchor>
                				<ul>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="Editor">Editor</g:Anchor></li>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="Magnets">Magnets</g:Anchor></li>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="DST">Logical</g:Anchor></li>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="Database">Database</g:Anchor></li>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="AdminPage">Admin</g:Anchor>
					<ul>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="logManage">Logical Management</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="logCreation">Logical Creation</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="magManage">Magnet Management</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="magCreation">Magnet Creation</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="proManage">Programming Management</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="stuManage">Student Management</g:Anchor></li>
					    <li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="review">Review Problems</g:Anchor></li>
					</ul></li>
					<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}">User</g:Anchor>
						<ul>
							<li><g:Anchor addStyleNames="{style.anchor} {style.hello-logout}" ui:field="logout">Logout</g:Anchor></li>
						</ul></li>
				</ul></g:HTMLPanel>
				</g:SimplePanel>
		</g:north>
		<g:center>
		<g:AbsolutePanel></g:AbsolutePanel>
	  	</g:center>
     
		</g:DockLayoutPanel>

 </ui:UiBinder> 